	

<mvc:View controllerName="hac2build.purchaseorderanalysis.controller.PurchaseOrderAnalysis"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
	xmlns:plugins="sap.m.plugins"
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
	xmlns:smartTable="sap.ui.comp.smarttable">
	<Page id="_IDGenPage1" showHeader="false" enableScrolling="true">
	<content>
		<Table class="table1" items="{oTableModel>/}"  id="idTablelist" visible="true"
				sticky="HeaderToolbar,InfoToolbar,ColumnHeaders" mode="SingleSelectMaster" selectionChange="onSelectedRow" alternateRowColors="true">
				<headerToolbar backgroundDesign="Solid">
					<OverflowToolbar id="_IDGenOverflowToolbar1">
						<content>
							<!-- <Title id="_IDGenTitle1" text="Purchase Order Analysis" level="H2"/> -->
							<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
							<Button id="_IDGenButton3" text="Local AI" press="onLocalAI"/>
							<Button id="_IDGenButton1" text="Availibility Check" press="onAvailablePress"/>
							<Button id="_IDGenButton2" text="Notify Customer" press="onEmailPress"/>
						</content>
					</OverflowToolbar>
				</headerToolbar>
				<columns>
					<Column id="_IDGenColumn1" hAlign="Center">
						<Label id="_IDGenLabel1" design="Bold" text="Supplier"/>
					</Column>
					<Column id="_IDGenColumn2" hAlign="Center">
						<Label id="_IDGenLabel2" design="Bold" text="PO"/>
					</Column>
					<Column id="_IDGenColumn3" hAlign="Center">
						<Label id="_IDGenLabel3" design="Bold" text="PO Item"/>
					</Column>
					<Column id="_IDGenColumn4" hAlign="Center">
						<Label id="_IDGenLabel4" design="Bold" text="PO SLN"/>
					</Column>
					<Column id="_IDGenColumn10" hAlign="Center">
						<Label id="_IDGenLabel10" design="Bold" text="Material"/>
					</Column>
					<Column id="_IDGenColumn11" hAlign="Center">
						<Label id="_IDGenLabel11" design="Bold" text="Description"/>
					</Column>
					<Column id="_IDGenColumn12" hAlign="Center">
						<Label id="_IDGenLabel12" design="Bold" text="QTY"/>
					</Column>
					<!-- <Column id="_IDGenColumn5" hAlign="Center">
						<Label id="_IDGenLabel5" design="Bold" text="FG Item"/>
					</Column> -->
					<Column id="_IDGenColumn13" hAlign="Center">
						<Label id="_IDGenLabel13" design="Bold" text="Plant"/>
					</Column>
					<Column id="_IDGenColumn14" hAlign="Center">
						<Label id="_IDGenLabel14" design="Bold" text="Sales Order"/>
					</Column>
					<Column id="_IDGenColumn6" hAlign="Center">
						<Label id="_IDGenLabel6" design="Bold" text="SO Item"/>
					</Column>
					<!-- <Column id="_IDGenColumn15" hAlign="Center">
						<Label id="_IDGenLabel15" design="Bold" wrapping="true" text="Purchase Orgnisation"/>
					</Column>
					<Column id="_IDGenColumn7" hAlign="Center">
						<Label id="_IDGenLabel7" design="Bold" wrapping="true" text="Purchasing Group"/>
					</Column> -->
					<Column id="_IDGenColumn8" hAlign="Center" autoPopinWidth="50">
						<Label id="_IDGenLabel8" design="Bold" wrapping="true" text="Planned Delivery Date"/>
					</Column>
					<Column id="_IDGenColumn16" hAlign="Center">
						<Label id="_IDGenLabel16" design="Bold" wrapping="true" text="Local AI Delivery Date"/>
					</Column>
					<Column id="_IDGenColumn9" hAlign="Center">
						<Label id="_IDGenLabel9" design="Bold" wrapping="true" text="GEN AI Delivery Date"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="_IDGenColumnListItem1" class="cells">
						<cells >
							<ObjectIdentifier id="_IDGenObjectIdentifier1" title="{oTableModel>Supplier}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier2" title="{oTableModel>PO}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier3" title="{oTableModel>PO_Item}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier4" title="{oTableModel>PO_SL}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier5" title="{oTableModel>Material}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier6" title="{oTableModel>Material_Description}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier7" title="{oTableModel>Quantity} PC"/>
							<!-- <ObjectIdentifier id="_IDGenObjectIdentifier8" title="{FG_Item}"/> -->
							<ObjectIdentifier id="_IDGenObjectIdentifier9" title="{oTableModel>Plant}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier10" title="{oTableModel>Sales_order}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier11" title="{oTableModel>SO_Item}"/>
							<!-- <ObjectIdentifier id="_IDGenObjectIdentifier12" title="{Purchase_Orgnisation}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier13" title="{Purchasing_Group}"/> -->
							<ObjectIdentifier id="_IDGenObjectIdentifier14" title="{oTableModel>Delivery_date}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier15" title="{oTableModel>Local_AI_Delivery_Date}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier16" title="{oTableModel>GEN_AI_Delivery_Date}"/>

						</cells>
					</ColumnListItem>
				</items>
			</Table>
	</content>
	</Page>
</mvc:View>

