   <mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:plugins="sap.m.plugins"
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
	xmlns:smartTable="sap.ui.comp.smarttable"
	controllerName="hac2build.salesorderanalysis.controller.salesOrderView"
	height="100%">
<Page id="_IDGenPage1" showHeader="false" enableScrolling="true">
	<content>
		<HBox id="_IDGenHBox1" backgroundDesign="Solid" height="50px">
			<VBox id="_IDGenVBox1" class="sapUiXSMarginBegin sapUiSmallMargin headerInner">
					
			</VBox>
		</HBox>
	</content>
	<content>
		<Table class="table1" items="{oTableModel>/}"  id="idTablelist" visible="true"
				sticky="HeaderToolbar,InfoToolbar,ColumnHeaders" mode="SingleSelectMaster" selectionChange="onSelectedRow" alternateRowColors="true">
				<headerToolbar backgroundDesign="Solid">
					<OverflowToolbar id="_IDGenOverflowToolbar1">
						<content>
							<Title id="_IDGenTitle1" text="Sales Order Analysis" level="H2"/>
							<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
							<Button id="_IDGenButton1" text="Availibility Check" press="onAvailablePress"/>
							<Button id="_IDGenButton2" text="Notify Customer" press="onEmailPress"/>
						</content>
					</OverflowToolbar>
				</headerToolbar>
				<columns>
					<Column id="_IDGenColumn1" hAlign="Center">
						<Label id="_IDGenLabel1" design="Bold" text="Sales Order"/>
					</Column>
					<Column id="_IDGenColumn2" hAlign="Center">
						<Label id="_IDGenLabel2" design="Bold" text="Sold-To-Party"/>
					</Column>
					<Column id="_IDGenColumn3" hAlign="Center">
						<Label id="_IDGenLabel3" design="Bold" text="Item"/>
					</Column>
					<Column id="_IDGenColumn4" hAlign="Center">
						<Label id="_IDGenLabel4" design="Bold" text="Plant"/>
					</Column>
					<Column id="_IDGenColumn5" hAlign="Center">
						<Label id="_IDGenLabel5" design="Bold" text="Material"/>
					</Column>
					<Column id="_IDGenColumn6" hAlign="Center">
						<Label id="_IDGenLabel6" design="Bold" text="Quantity"/>
					</Column>
					<Column id="_IDGenColumn7" hAlign="Center">
						<Label id="_IDGenLabel7" design="Bold" text="Value"/>
					</Column>
					<Column id="_IDGenColumn8" hAlign="Center" autoPopinWidth="50">
						<Label id="_IDGenLabel8" design="Bold" wrapping="true" text="Current SAP Delivery Date"/>
					</Column>
					<Column id="_IDGenColumn9" hAlign="Center">
						<Label id="_IDGenLabel9" design="Bold" wrapping="true" text="Gen AI Predicted Delivery Date"/>
					</Column>
					<Column id="_IDGenColumn10" hAlign="Center" autoPopinWidth="50">
						<Label id="_IDGenLabel10" design="Bold" wrapping="true" text="Sentiment %"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem id="_IDGenColumnListItem1" class="cells" press="onRowSelected">
						<cells >
							<ObjectIdentifier id="_IDGenObjectIdentifier1" title="{oTableModel>Sales_Order}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier2" title="{oTableModel>Sold_to}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier3" title="{oTableModel>SO_Item}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier4" title="{oTableModel>Plant}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier5" title="{oTableModel>Material}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier6" title="{oTableModel>Order_Quantity}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier7" title="{oTableModel>Unit_Price}"/>
							<ObjectIdentifier id="_IDGenObjectIdentifier8" title="{oTableModel>Current_SAP_Delivery_Date}"/>			
							<ObjectStatus id="_IDGenObjectStatus1" text="{oTableModel>GEN_AI_Delivery_Date}" />
							<!-- <ObjectStatus id="_IDGenObjectStatus1" state="{path:'oTableModel>GEN_AI_Delivery_Date',formatter:'.formatter.formatDateFn'}"/> -->
							<!-- <ObjectStatus id="_IDGenObjectStatus1" state="oTableModel>GEN_AI_Delivery_Date"/> -->
							<!-- <ObjectStatus id="_IDGenObjectStatus1" text="{Delivery_Date}"/> -->
							<ObjectStatus id="_IDGenObjectStatus2" text="{oTableModel>Sentiment}"/>
							
						</cells>
					</ColumnListItem>
				</items>
			</Table>
	</content>
	</Page>
</mvc:View>
